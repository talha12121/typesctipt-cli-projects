#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//user/bin/env" ]; then
  exec "$basedir//user/bin/env" node "$basedir/../talharahim_calculator/index.js" "$@"
else 
  exec /user/bin/env node "$basedir/../talharahim_calculator/index.js" "$@"
fi
